  m_Name: "\uC218\uB825\uBC1C\uC804\uC18C\uC9C4\uC785"
  m_EditorClassIdentifier: 
  EventID: "\uC218\uB825\uBC1C\uC804\uC18C\uC9C4\uC785"
  EventText: "\uB2F9\uC2E0\uC774 \uC12C\uC5D0 \uC9C4\uC785\uD55C \uC21C\uAC04\uBD80\uD130
    \uC758\uB3C4\uC801\uC73C\uB85C \uBB34\uC2DC\uD558\uACE0 \uC788\uB358 \uB300\uC0C1.
    \uD558\uC9C0\uB9CC, \uC2DC\uC57C\uC5D0\uC11C \uC810\uC810 \uC874\uC7AC\uAC10\uC744
    \uD0A4\uC6CC \uC624\uB294 \uADF8\uAC83\uC740, \uB354 \uC774\uC0C1 \uB2F9\uC2E0\uC774
    \uBB34\uC2DC\uD560 \uC218 \uC5C6\uB294 \uC218\uC900\uC774 \uB418\uC5C8\uC2B5\uB2C8\uB2E4.\r\n\r\n...
    \uC628\uD1B5 \uD478\uB978\uC0C9\uC73C\uB85C \uB4A4\uB36E\uC778 \uC0B0\uC5D0 \uAC11\uC790\uAE30
    \uD68C\uC0C9 \uAC74\uCD95\uBB3C\uC774\uB77C\uB2C8! \uB204\uAC00 \uBD10\uB3C4
    \uC218\uC0C1\uD574 \uBCF4\uC774\uC9C0 \uC54A\uB098\uC694?\r\n\uC758\uBB38\uC744
    \uAC10\uCD9C \uC218 \uC5C6\uB358 \uB2F9\uC2E0\uC740 \uD68C\uC0C9 \uAC74\uCD95\uBB3C
    \uCABD\uC73C\uB85C \uBC1C\uAE38\uC744 \uB3CC\uB9BD\uB2C8\uB2E4. \uAC74\uCD95\uBB3C\uC758
    \uC804\uCCB4 \uD615\uD0DC\uAC00 \uC5B4\uB290 \uC815\uB3C4 \uBCF4\uC774\uAE30
    \uC2DC\uC791\uD560 \uBB34\uB835, \uB2F9\uC2E0\uC740 \uADF8\uAC83\uC758 \uC815\uCCB4\uB97C
    \uC54C \uC218 \uC788\uAC8C \uB418\uC5C8\uC2B5\uB2C8\uB2E4.\r\n\r\n\uADF8 \uC778\uACF5\uC801\uC778
    \uAC74\uCD95\uBB3C\uC740, \uB2E4\uB984\uC774 \uC544\uB2C8\uB77C \uC218\uB825
    \uBC1C\uC804\uC18C\uC600\uC2B5\uB2C8\uB2E4! \uC218\uB825 \uBC1C\uC804\uC18C\uB77C\uB2C8,
    \uC774\uAC8C \uC6EC \uB5A1\uC77C\uAE4C\uC694? \uBB3C\uB860 \uAC70\uC810\uC73C\uB85C
    \uAC00\uC838\uAC08 \uC21C \uC5C6\uACA0\uC9C0\uB9CC, \uC774\uB807\uAC8C \uBB3C\uB85C
    \uAC00\uB4DD\uCC2C \uC138\uC0C1\uC5D0\uC11C \uC218\uB825 \uBC1C\uC804\uC18C\uB9CC\uD07C
    \uC774\uB4DD\uC744 \uBCF4\uB294 \uC2DC\uC124\uC774 \uB610 \uC788\uACA0\uB0D0\uB294
    \uB9D0\uC785\uB2C8\uB2E4! \uBA40\uCA61\uD55C \uC0DD\uC874\uC790 \uADF8\uB8F9\uC744
    \uB9CC\uB0A0 \uC218\uB3C4 \uC788\uACE0, \uC5B4\uCA4C\uBA74 \uC804\uAE30 \uBB38\uC81C\uB97C
    \uD574\uACB0\uD560 \uC218 \uC788\uC744\uC9C0\uB3C4 \uBAB0\uB77C\uC694!\r\n\r\n\uBD80\uD47C
    \uAFC8\uC744 \uC548\uACE0, \uB2F9\uC2E0\uC740 \uC218\uB825 \uBC1C\uC804\uC18C\uC758
    \uC785\uAD6C\uB97C \uBC14\uB77C\uBD05\uB2C8\uB2E4. \uC785\uAD6C\uC5D0\uB294,
    \uD070 \uCD1D\uC744 \uB4E0 \uB450 \uC0AC\uB78C\uC774 \uC8FC\uBCC0\uC744 \uACBD\uACC4\uD558\uBA70
    \uC794\uB729 \uAE34\uC7A5\uD55C \uC0C1\uD0DC\uB85C \uC11C \uC788\uB124\uC694.
    \uB5A8\uB9AC\uB294 \uC190\uC744 \uBCF4\uC544\uD558\uB2C8, \uC798\uBABB \uB2E4\uAC00\uAC14\uB2E4\uAC04
    '\uC2E4\uC218\uB85C' \uB2F9\uC2E0\uC744 \uC3F4\uBC84\uB9B4\uC9C0\uB3C4 \uBAA8\uB985\uB2C8\uB2E4."
